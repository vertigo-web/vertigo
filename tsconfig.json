{
    "compilerOptions": {
        "target": "esnext",
        "module": "es6",
        "strict": true,
        "jsx": "react",
        "lib": ["dom", "es6", "esnext"],
        "downlevelIteration": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noUncheckedIndexedAccess": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "baseUrl": ".",
        "noEmit": true
    }
}
